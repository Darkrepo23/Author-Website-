            "content": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n<head>\r\n  <meta charset=\"UTF-8\" />\r\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\r\n  <title>Admin | Herbert Schildt</title>\r\n  <link rel=\"stylesheet\" href=\"admin.css\" />\r\n  <link rel=\"preload\" href=\"../font/Encode_Sans/static/EncodeSans-Regular.ttf\" as=\"font\" type=\"font/ttf\" crossorigin>\r\n  <link rel=\"preload\" href=\"../font/Encode_Sans/static/EncodeSans-SemiBold.ttf\" as=\"font\" type=\"font/ttf\" crossorigin>\r\n</head>\r\n<body>\r\n  <aside class=\"sidebar\">\r\n    <div class=\"brand\">\r\n      <div class=\"brand-box\">\r\n        <img src=\"Group 10.png\" alt=\"Herbert Schildt\" class=\"brand-logo\" />\r\n      </div>\r\n    </div>\r\n    <button class=\"burger\" id=\"burger\" aria-label=\"Toggle menu\">\r\n      <span></span><span></span><span></span>\r\n    </button>\r\n    <nav class=\"menu\">\r\n      <button class=\"menu-item active\" data-target=\"dashboard\">Dashboard</button>\r\n      <button class=\"menu-item\" data-target=\"books\">Book Management</button>\r\n      <button class=\"menu-item\" data-target=\"users\">Manage Users</button>\r\n    </nav>\r\n    <div class=\"admin-profile\">\r\n      <div class=\"avatar\" aria-hidden=\"true\"></div>\r\n      <div class=\"admin-name\">Admin</div>\r\n      <a href=\"../Login/pages/login.html\" class=\"logout\">Log Out</a>\r\n    </div>\r\n  </aside>\r\n\r\n  <main class=\"content\">\r\n    <!-- DASHBOARD -->\r\n    <section id=\"dashboard\" class=\"section visible\">\r\n      <h1>Dashboard</h1>\r\n      <div class=\"cards\">\r\n        <div class=\"card\">\r\n          <div class=\"card-num\" id=\"stat-total-books\">0</div>\r\n          <div class=\"card-label\">Total books listed</div>\r\n        </div>\r\n        <div class=\"card\">\r\n          <div class=\"card-num\" id=\"stat-pdf\">0</div>\r\n          <div class=\"card-label\">PDF BOOK</div>\r\n        </div>\r\n        <div class=\"card\">\r\n          <div class=\"card-num\" id=\"stat-physical\">0</div>\r\n          <div class=\"card-label\">Physical BOOK</div>\r\n        </div>\r\n        <div class=\"card\">\r\n          <div class=\"card-num\" id=\"stat-orders\">0</div>\r\n          <div class=\"card-label\">Order Summary</div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- BOOK MANAGEMENT -->\r\n    <section id=\"books\" class=\"section\">\r\n      <div class=\"section-head\">\r\n        <h1>Book Management</h1>\r\n        <button class=\"primary\" id=\"btn-open-add-book\">\r\n          <span class=\"icon-book\" aria-hidden=\"true\"></span>\r\n          Add Book\r\n        </button>\r\n      </div>\r\n      <div class=\"table-wrap\">\r\n        <table class=\"table\">\r\n          <thead>\r\n            <tr>\r\n              <th>Book Title</th>\r\n              <th>Category</th>\r\n              <th>Price</th>\r\n              <th>Book Type</th>\r\n              <th>Edit</th>\r\n              <th>Delete</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody id=\"books-tbody\"></tbody>\r\n        </table>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- USERS -->\r\n    <section id=\"users\" class=\"section\">\r\n      <h1>Manage Users</h1>\r\n      <div class=\"table-wrap\">\r\n        <table class=\"table\">\r\n          <thead>\r\n            <tr>\r\n              <th>Full Name</th>\r\n              <th>Email</th>\r\n              <th>Purchase Books</th>\r\n              <th>Date Joined</th>\r\n              <th>View</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody id=\"users-tbody\"></tbody>\r\n        </table>\r\n      </div>\r\n    </section>\r\n  </main>\r\n\r\n  <!-- Add/Edit Book Modal -->\r\n  <div class=\"modal\" id=\"book-modal\" aria-hidden=\"true\" role=\"dialog\" aria-labelledby=\"book-modal-title\">\r\n    <div class=\"modal-card\">\r\n      <div class=\"modal-head\">\r\n        <h2 id=\"book-modal-title\">Add New Books</h2>\r\n        <button class=\"icon-close\" id=\"book-modal-close\" aria-label=\"Close\"></button>\r\n      </div>\r\n      <div class=\"modal-body add-book-grid\">\r\n        <div class=\"book-image\">\r\n         \r\n          <label class=\"secondary file-btn\">\r\n            <input type=\"file\" id=\"book-cover\" accept=\"image/*\" hidden />\r\n            Upload Image Book\r\n          </label>\r\n          <div class=\"field\">\r\n            <label>Book Type</label>\r\n            <select id=\"book-type\">\r\n              <option value=\"PDF\">PDF BOOK</option>\r\n              <option value=\"PHYSICAL\">PHYSICAL BOOK</option>\r\n            </select>\r\n          </div>\r\n          <label class=\"secondary file-btn\">\r\n            <input type=\"file\" id=\"book-pdf\" accept=\"application/pdf\" hidden />\r\n            Upload PDF\r\n          </label>\r\n        </div>\r\n        <form id=\"book-form\" class=\"book-form\" autocomplete=\"off\">\r\n          <div class=\"field\">\r\n            <label>Title</label>\r\n            <input id=\"book-title\" type=\"text\" required />\r\n          </div>\r\n          <div class=\"field\">\r\n            <label>Description</label>\r\n            <textarea id=\"book-description\" rows=\"6\"></textarea>\r\n          </div>\r\n          <div class=\"grid-3\">\r\n            <div class=\"field\">\r\n              <label>Price</label>\r\n              <input id=\"book-price\" type=\"number\" step=\"0.01\" min=\"0\" required />\r\n            </div>\r\n            <div class=\"field\">\r\n              <label>Category</label>\r\n              <input id=\"book-category\" type=\"text\" required />\r\n            </div>\r\n            <div class=\"field\">\r\n              <label>Rating</label>\r\n              <input id=\"book-rating\" type=\"number\" step=\"0.1\" min=\"0\" max=\"5\" />\r\n            </div>\r\n          </div>\r\n          <div class=\"grid-3\">\r\n            <div class=\"field\">\r\n              <label>Year</label>\r\n              <input id=\"book-year\" type=\"number\" min=\"1900\" max=\"2100\" />\r\n            </div>\r\n            <div class=\"field\">\r\n              <label>Pages</label>\r\n              <input id=\"book-pages\" type=\"number\" min=\"1\" />\r\n            </div>\r\n            <div class=\"field\">\r\n              <label>ISBN</label>\r\n              <input id=\"book-isbn\" type=\"text\" />\r\n            </div>\r\n          </div>\r\n          <div class=\"modal-actions\">\r\n            <button type=\"button\" class=\"secondary\" id=\"book-cancel\">Go Back</button>\r\n            <button type=\"submit\" class=\"primary\" id=\"book-submit\">Add New Book</button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n  <!-- User Details Modal -->\r\n  <div class=\"modal\" id=\"user-modal\" aria-hidden=\"true\" role=\"dialog\" aria-labelledby=\"user-modal-title\">\r\n    <div class=\"modal-card\">\r\n      <div class=\"modal-head\">\r\n        <h2 id=\"user-modal-title\">User Information</h2>\r\n        <button class=\"icon-close\" id=\"user-modal-close\" aria-label=\"Close\"></button>\r\n      </div>\r\n      <div class=\"modal-body\">\r\n        <div class=\"user-info\">\r\n          <div class=\"line\"><span>Full Name:</span><b id=\"u-fullname\"></b></div>\r\n          <div class=\"line\"><span>Email:</span><b id=\"u-email\"></b></div>\r\n          <div class=\"line\"><span>Date Joined:</span><b id=\"u-joined\"></b></div>\r\n          <h3>Purchase Book</h3>\r\n          <div id=\"u-books\" class=\"chips\"></div>\r\n        </div>\r\n        <div class=\"modal-actions gap\">\r\n          <button class=\"secondary\" id=\"user-block\">Block Account</button>\r\n          <button class=\"danger\" id=\"user-delete\">Delete Account</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n  <script src=\"admin.js\"></script>\r\n</body>\r\n</html>\r\n\r\n"
        }
    ]
}